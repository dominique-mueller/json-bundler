{
	"name": "json-bundler",
	"description": "Bundles your JSON & JSON5 files intelligently.",
	"version": "1.0.0",
	"author": {
		"name": "Dominique MÃ¼ller",
		"email": "dominique.m.mueller@gmail.com",
		"url": "https://www.devdom.io/"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/dominique-mueller/json-bundler"
	},
	"homepage": "https://github.com/dominique-mueller/json-bundler",
	"bugs": {
		"url": "https://github.com/dominique-mueller/json-bundler/issues"
	},
	"keywords": [
		"json",
		"json5",
		"bundle",
		"merge",
		"combine",
		"18n",
		"angular"
	],
	"main": "index.js",
	"types": "index.d.ts",
	"bin": {
		"json-bundler": "bin/json-bundler.js"
	},
	"scripts": {
		"build": "tsc -p tsconfig.json && copyfiles \"bin/**\" dist",
		"clean": "rimraf -r dist/*",
		"pretest": "copyfiles \"./test/node-module-references/src/json-bundler-test/**\" \"./node_modules\" --up 3",
		"release": "automatic-release && copyfiles \"docs/**\" package.json CHANGELOG.md LICENSE README.md dist",
		"test:coverage": "codecov -f coverage/coverage-final.json",
		"test": "jest --config jest.config.json --runInBand --no-cache"
	},
	"dependencies": {
		"assign-deep": "0.4.x",
		"chalk": "2.4.x",
		"chokidar": "2.0.x",
		"fs-extra": "5.0.x",
		"json5": "1.0.x",
		"log-update": "2.3.x",
		"yargs": "11.0.x"
	},
	"devDependencies": {
		"@types/jest": "23.0.x",
		"@types/node": "9.6.x",
		"automatic-release": "1.1.x",
		"codecov": "3.0.x",
		"copyfiles": "2.0.x",
		"jest": "22.4.x",
		"rimraf": "2.6.x",
		"ts-jest": "22.4.x",
		"typescript": "2.8.x"
	}
}
